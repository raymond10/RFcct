#SchemaCompetence dt {
	text-align: left;
	cursor: pointer;
}

#SchemaCompetence dt.selected {
	background-position: right;
	background-image: url(img/ico_node_collapsed_rouge_16.png);
	background-repeat: no-repeat;
	font-weight: bold;
	color: red;
}

#SchemaCompetence dd {
	text-align: left;
	cursor: pointer;
	margin-left: 5px;
}

#SchemaCompetence dd.selected {
	font-weight: bold;
	color: red;
}

table.progressBarContainer {
	width: 100%;
	border-spacing: 0px;
}

table.progressBarContainer td {
	font-family: Helvetica;
	font-size: 8pt;
	padding: 2px;
}

table.progressBarContainer td.progressValue {
	white-space: nowrap;
}

.progressBar {
	border: 1px solid black;
}

.progressBar td.percentageFinished {
	background-image: url(img/progress-bar.gif);
	background-repeat: repeat-x;
}

table.progressBar td.percentageFinished.done {
	background-image: url(img/progress-bar.gif);
	background-repeat: repeat-x;
}

table.progressBar td.percentageUnfinished {
	background-image: url(img/progress-remainder.gif);
	background-repeat: repeat-x;
}

table.progressBarContainer td.progressMaximum {
	white-space: nowrap;
}

.etiquette {
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
