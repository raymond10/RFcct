/* Classes du wrapper horizontal */
.cktl_container_principal {
	width: 970px;
	margin-left: auto;
	margin-right: auto;
	text-align: center;
	float: inherit;
}

.cktl_entete_wrapper_horizontal {
	width: 970px;
	height: 30px;
	position: relative;
	font-family: Arial, Helvetica, sans-serif;
	text-align: center;
	font-size: 16pt;
	display: block;
	color: #000000;
	font-weight: bold;
	padding: 0;
}

.cktl_copyright_wrapper_horizontal {
	width: 970px;
	height: 15px;
	position: relative;
	font-family: Arial, Helvetica, sans-serif;
	font-size: 10pt;
	display: block;
	color: #000000;
	font-weight: bold;
}

.cktl_copyright_wrapper_horizontal a {
	text-decoration: none;
}

.cktl_accueil_wrapper_horizontal {
	width: 800px;
	height: 550px;
	font-family: Verdana, Geneva, Arial, Helvetica, sans-serif;
	text-align: left;
	margin: 0 auto 0 auto;
	padding: 0;
	font-size: 12px;
}

.cktl_accueil_wrapper_horizontal h3 {
	color: #9EADC4;
	text-align: left;
	padding-left: 5px;
	padding-bottom: 2px;
	margin: 0px;
	margin-top: 1px;
	font-size: large;
	border-bottom: 1px solid #9EADC4;
}

