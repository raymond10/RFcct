<webobject name = "Wrapper">
  <table width = "100%">
    <tr>
      <td id = "main" valign = "top" align = "center">
        <table cellpadding = "0" cellspacing = "0" width = "100%" style = "background-color:white;">
          <tbody>
            <tr>
              <td>
                <br />
                <br />
                <div class = "cktl_accueil_wrapper_horizontal">
                  <webobject name = "HasDroitConsultation">
                    <h4>Acc&egrave;s rapide par num&eacute;ro</h4>
                    <webobject name = "FormRechercher">
                      <webobject name = "CodeClassification" />-<webobject name = "NumeroCompetence" />
